/**index.wxss**/
page{
  background:#ebf0f6;
}
.slide-image {
  width:100%;
  height:100%;
}
.index-swiper .wx-swiper-dot{
    width:20rpx;
    display: inline-flex;
    height: 20rpx;
    margin-left: 10rpx;
    background-color:rgba(60, 197, 31, 0);
    justify-content:space-between;
}
.index-swiper .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    border:1px solid #fff;
    background-color:rgba(255, 255, 255, 1);
    border-radius: 100%
}
.index-swiper .wx-swiper-dot-active::before{
    border:1px solid #3CC51F;
    background:rgba(60, 197, 31, 1);
}
.yuesaoiconls{
  width:100%;
  height:auto;
  overflow: hidden;
  padding:20rpx 0;
  margin:0 auto;
  margin-bottom:20rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  flex-direction: row;
  background:#fff;
}
.yuesaoiconls .icondiv{
  flex-grow: 1;
  min-width:20%;
  max-width:20%;
  text-align: center;
  padding:0 20rpx;
  box-sizing: border-box;
}
.yuesaoiconls .icondiv image{
  width:100%;
  border-radius:100%;
  -webkit-border-radius:100%;
}
.yuesaoiconls .icondiv text{
  color:#585858;
  font-size:25rpx;
}
.searchys{
  width:100%;
  height:auto;
  overflow:hidden;
  background:#fff;
  border:1px solid #e6eaeb;
  border-left:0;
  border-right:0;
  text-align: center;
  padding-bottom:15rpx;
}
.searchys .top{
  width:90%;
  height:85rpx;
  line-height:85rpx;
  font-size:30rpx;
  color:#000;
  position: relative;
  text-align: left;
  margin:0 auto;
}
.searchys .top image{
  position: absolute;
  top:20rpx;
  right:0;
  width:18rpx;
}
.searchys .centerimg{
  width:90%;
  margin:0 auto;
  position: relative;
}
.searchys .centerimg .shangbiao{
  position: absolute;
  right:0;
  top:0;
}
.searchys .centerimg .shangbiao::after{
  width:70rpx;
  height:70rpx;
  content: ' ';
  position: absolute;
  right:-73rpx;
  top:-73rpx;
  border-top:40rpx solid rgba(204, 27, 35, 0);
  border-left:40rpx solid rgba(204, 27, 35, 0);
  border-bottom:40rpx solid rgba(204, 27, 35, 0);
  border-right:40rpx solid rgba(204, 27, 35, 1);
  transform: rotate(-225deg);
  -webkit-transform: rotate(-225deg);
}
.searchys .centerimg .shangbiao text{
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  position:absolute;
  right:18rpx;
  top:20rpx;
  z-index:2;
  color:#fff;
  font-size:24rpx;
}
.searchys .centerimg image{
  width:100%;
}
.searchys .centerimg .textdiv1{
  width:100%;
  height:45rpx;
  line-height:45rpx;
  color:#000;
  display: flex;
  -webkit-display:flex;
  justify-content: flex-start;
  flex-direction: row;
  font-size:24rpx;
  margin:5px auto;
}
.searchys .centerimg .textdiv1 .left{
  color:#000;
  font-size:28rpx;
  flex:3;
  text-align: left;
}
.searchys .centerimg .textdiv1 .right{
  color:#d9a300;
  font-size:28rpx;
  font-weight:800;
  flex:4;
  text-align: left;
}
.searchys .centerimg .textdiv1 .right text{
  color:#5b5b5b;
  font-size:18rpx;
  font-weight: normal;
}
.searchys .centerimg .textdiv1.color{
  color:#6f6f6f;
}
.mianshidiv{
  width:100%;
  height:auto;
  overflow:hidden;
  padding:20rpx 0;
  padding-top:0;
  border-top:1px solid #e9e9e9;
  border-bottom:1px solid #e9e9e9;
  background:#fff;
  margin:20rpx auto;
}
.mianshidiv .title{
  width:100%;
  height:35px;
  line-height:35px;
  overflow:hidden;
  padding:0 5%;
  color:#000;
  font-size:20rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-content: flex-start;
  flex-direction: row;
  margin:0 auto;
  text-align: left;
  box-sizing:border-box;
  -webkit-box-sizing:border-box;
  border-bottom:1px solid #e9e9e9;
  position: relative;
}
.mianshidiv .title .left{
  height:35px;
  line-height:35px;
  flex:1;
  font-size:33rpx;
}
.mianshidiv .title .right{
  height:35px;
  line-height:35px;
  flex:1;
  font-size:26rpx;
  text-align: right;
}
.mianshidiv .title.textadd{
  font-size:26rpx;
}
.mianshidiv .title text{
  display:block;
  font-size:26rpx;
}
.mianshidiv .title view{
  width:90%;
}
.mianshidiv .title >  image{
  width:40rpx;
  align-self: center;
  margin-right:10rpx;
}
.mianshidiv .inputview, picker{
  width:100%;
  height:65rpx;
  line-height:65rpx;
  margin:40rpx auto;
  display: flex;
  display: -webkit-flex;
  font-size:25rpx;
  color:#000;
  flex-direction: row;
  align-items:center;
}
.mianshidiv .inputview .left{
  flex:3;
  text-align: right;
}
.mianshidiv .inputview .right{
  flex:9;
  text-align: right
}
.mianshidiv .inputview .right input, picker{
  border:1px solid #e9e9e9;
  flex:6;
  width:90%;
  height:80rpx;
  margin-left:5%;
  text-align: left;
  padding-left: 1em;
  box-sizing: border-box;
  -webkit-box-sizing:border-box;
  border-radius:10rpx;
  -webkit-border-radius:10rpx;
}
.mianshidiv button{
  width:40%;
  height:80rpx;
  line-height:80rpx;
  background:#3CC51F;
  color:#fff;
  text-align: center;
  font-size:30rpx;
  margin:20rpx auto;
}
.chengnuotext{
  width:80%;
  height:auto;
  line-height:30rpx;
  text-align: center;
  font-size:20rpx;
  color:#939393;
  margin:20rpx auto;
}
.chengnuotext b{
  color:#ff0619;
}